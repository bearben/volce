(set-logic QF_LIA)
(set-info :test.smt2)
(set-info :smt-lib-version 2.0)
(set-info :status sat)
(declare-fun x () Int)
(declare-fun y () Int)
(declare-fun a () Bool)
(declare-fun b () Bool)
(declare-fun c () Bool)
(assert (or (xor a b) (xor a c) (xor b c)))
(assert (and (xor a b c (<= (+ x (* 2 y)) 1)) (< x 2) (<= y 2) (> x 0) (>= y 0)))
(check-sat)
(exit)
