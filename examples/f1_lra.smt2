(set-logic QF_LRA)
(set-info :f1.smt2)
(set-info :smt-lib-version 2.0)
(set-info :status sat)
(declare-fun x () Real)
(declare-fun y () Real)
(assert (and (<= x 1) (<= y 1) (>= x 0) (>= y 0)))
(declare-fun b () Bool)
(assert (let ((v1 (< (+ x y) 1)) (v2 (< x y))) 
	(and (or v1 (not v2)) (or v1 v2 b) (or (not v1) v2))))
(check-sat)
(exit)
